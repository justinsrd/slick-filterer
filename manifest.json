{
	"name": "slick-filterer",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Filters slickdeals posts based on rating",
	"permissions": [],
	"content_scripts": [
		{
			"matches": ["*://*.slickdeals.net/*"],
			"css": ["popup.css"],
			"js": ["lib/jquery-3.2.1.min.js", "popup.js"]
		}
	],
	"web_accessible_resources": [
       "slickfilter.html"
     ]
}
